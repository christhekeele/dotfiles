{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "css",
    "git-commit",
    "graphql",
    "handlebars",
    "html",
    "jade",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "plaintext",
    "pug",
    "restructuredtext",
    "scss",
    "text",
    "yaml",
    "yml"
  ],
  "cSpell.enableFiletypes": [
    "!python",
    "!elixir",
    "!ruby",
    "!json",
    "!javascript",
    "!typescript",
  ],
  "cSpell.userWords": [
    "agnostically",
    "backpressure",
    "behaviour",
    "behaviours",
    "debugpy",
    "interactable",
    "moreso",
    "overrwrite",
    "rabbitmq",
    "runtimes",
    "structlog",
    "unintuitive",
    "Upsert",
    "whiteboarding"
  ],
  "editor.acceptSuggestionOnEnter": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "'Monaspace Argon', 'Fira Code', monospace",
  "editor.fontLigatures": "'calt', 'liga', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08'",
  "editor.fontSize": 16,
  "editor.fontWeight": "600",
  "editor.largeFileOptimizations": false,
  "editor.renderWhitespace": "none",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordBasedSuggestions": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.config": "text",
    "*.exs": "elixir",
    "*.heex": "phoenix-heex",
    "*.livemd": "markdown",
    "\\.?env(\\..*?)?$": "shell"
  },
  "files.exclude": {
    "**/.elixir_ls": true,
    "**/node_modules": true
  },
  "git.openRepositoryInParentFolders": "never",
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.statusBar.pullRequests.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown-preview-enhanced.codeBlockTheme": "github.css",
  "markdown-preview-enhanced.enableTypographer": true,
  "markdown-preview-enhanced.frontMatterRenderingOption": "code block",
  "markdown-preview-enhanced.previewTheme": "github-light.css",
  "markdown-preview-enhanced.revealjsTheme": "night.css",
  "markdown-preview-enhanced.usePandocParser": true,
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "**/deps": true,
    "**/node_modules": false,
    "node_modules": true
  },
  "search.useGlobalIgnoreFiles": true,
  "security.workspace.trust.enabled": false,
  "svg.preview.mode": "svg",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorCustomizations": {
    "[Community Material Theme Darker High Contrast]": {},
    "editorBracketHighlight.foreground1": "#CDA869",
    "editorBracketHighlight.foreground2": "#CDA869",
    "editorBracketHighlight.foreground3": "#CDA869",
    "editorBracketHighlight.foreground4": "#CDA869",
    "editorBracketHighlight.foreground5": "#CDA869",
    "editorBracketHighlight.foreground6": "#CDA869",
    "editorBracketHighlight.unexpectedBracket.foreground": "#db6165"
  },
  "workbench.colorTheme": "Predawn Twilight",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "vs-minimal",
  "workbench.preferredDarkColorTheme": "Predawn Twilight",
  "workbench.startupEditor": "none"
}
