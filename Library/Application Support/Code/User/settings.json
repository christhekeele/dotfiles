{
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.fontSize": 14,
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontLigatures": true,
  "search.useGlobalIgnoreFiles": true,
  "workbench.preferredDarkColorTheme": "Predawn Twilight",
  "workbench.colorCustomizations": {
    "[Community Material Theme Darker High Contrast]": {}
  },
  "workbench.editor.enablePreview": false,
  "markdown-preview-enhanced.codeBlockTheme": "twilight.css",
  "markdown-preview-enhanced.revealjsTheme": "night.css",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "Upsert",
    "agnostically",
    "backpressure",
    "behaviour",
    "behaviours",
    "debugpy",
    "moreso",
    "overrwrite",
    "rabbitmq",
    "runtimes",
    "structlog",
    "unintuitive",
    "whiteboarding"
  ],
  "markdown-preview-enhanced.enableTypographer": true,
  "markdown-preview-enhanced.frontMatterRenderingOption": "code block",
  "markdown-preview-enhanced.previewTheme": "monokai.css",
  "markdown-preview-enhanced.usePandocParser": true,
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "search.exclude": {
    "**/deps": true,
    "**/node_modules": false,
    "node_modules": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.disableLanguages": ["markdown"],
  "editor.fontWeight": "600",
  "files.associations": {
    "*.exs": "elixir",
    "*.config": "text"
  },
  "python.showStartPage": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "material-icon-theme.showWelcomeMessage": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.statusBar.pullRequests.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "files.exclude": {
    "**/.elixir_ls": true,
    "**/node_modules": true
  },
  "window.zoomLevel": 1,
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "cSpell.enableFiletypes": [
    "!python",
    "!elixir",
    "!ruby"
  ],
  "python.languageServer": "Pylance",
  "editor.renderWhitespace": "none",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "css",
    "git-commit",
    "graphql",
    "handlebars",
    "html",
    "jade",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "plaintext",
    "pug",
    "restructuredtext",
    "scss",
    "text",
    "yaml",
    "yml"
  ],
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "Predawn Twilight",
  "vsicons.dontShowNewVersionMessage": true
}
